<%- include("partials/header") %>



    <% blogs.forEach(function(blog){ %>
        <div>
            <h2>
                <%= blog.title %>
            </h2>
            <img src="<%= blog.image %>" alt=""><br>
            <span><%= blog.created %></span>
            <p>
                <%= blog.body.substring(0, 100) %>...
            </p>
            <a href="/blogs/<%= blog._id %>">Show More</a>
        </div>
        <% }) %>
            <%- include("partials/footer") %>